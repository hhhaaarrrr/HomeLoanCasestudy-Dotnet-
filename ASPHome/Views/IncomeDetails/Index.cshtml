    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ProperityLocation)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProperityName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ESAmount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TypeofEmployeement)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Retirementage)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Organizationtype)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmployerName)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.ApplicationId }) |
                @Html.ActionLink("Details", "Details", new { id = item.ApplicationId }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.ApplicationId })

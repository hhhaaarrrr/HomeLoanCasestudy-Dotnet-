<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ProperityLocation)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProperityName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ESAmount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TypeofEmployeement)
